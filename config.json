{
    "log": {
    	"access": "/var/log/ProjectName/access.log",
        "error": "/var/log/ProjectName/error.log",
        "loglevel": "warning"
    },
    "inbounds": [
        {
            "port": 443,
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "LessXtcpID", // 填写你的 UUID
                        "flow": "xtls-rprx-direct",
                        "level": 0,
                        "email": "admin@DomainName"
                    }
                ],
                "decryption": "none",
                "fallbacks": [
                    {
                        "dest": TrojanPort, // 默认回落到 Xray 的 Trojan 协议
                        "xver": 1
                    },
                    {
                        "path": "/LessWsPath", // 必须换成自定义的 PATH
                        "dest": LessWsPort,
                        "xver": 1
                    },
                    {
                        "path": "/MessTcpPath", // 必须换成自定义的 PATH
                        "dest": MessTcpPort,
                        "xver": 1
                    },
                    {
                        "path": "/MessWsPath", // 必须换成自定义的 PATH
                        "dest": MessWsPort,
                        "xver": 1
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "xtls",
                "xtlsSettings": {
                    "alpn": [
                        "http/1.1"
                    ],
                    "certificates": [
                        {
                            "certificateFile": "/root/.acme.sh/DomainName/fullchain.cer", // 换成你的证书，绝对路径
                            "keyFile": "/root/.acme.sh/DomainName/DomainName.key" // 换成你的私钥，绝对路径
                        }
                    ]
                }
            }
        },
        {
            "port": TrojanPort,
            "listen": "127.0.0.1",
            "protocol": "trojan",
            "settings": {
                "clients": [
                    {
                        "password": "TrojanPass", // 填写你的密码
                        "level": 0,
                        "email": "admin@DomainName"
                    }
                ],
                "fallbacks": [
                    {
                        "dest": 80 // 或者回落到其它也防探测的代理
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "none",
                "tcpSettings": {
                    "acceptProxyProtocol": true
                }
            }
        },
        {
            "port": LessWsPort,
            "listen": "127.0.0.1",
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "LessWsID", // 填写你的 UUID
                        "level": 0,
                        "email": "admin@DomainName"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "ws",
                "security": "none",
                "wsSettings": {
                    "acceptProxyProtocol": true, // 提醒：若你用 Nginx/Caddy 等反代 WS，需要删掉这行
                    "path": "/LessWsPath" // 必须换成自定义的 PATH，需要和分流的一致
                }
            }
        },
        {
            "port": MessTcpPort,
            "listen": "127.0.0.1",
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {
                        "id": "MessTcpID", // 填写你的 UUID
                        "level": 0,
                        "email": "admin@DomainName"
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "none",
                "tcpSettings": {
                    "acceptProxyProtocol": true,
                    "header": {
                        "type": "http",
                        "request": {
                            "path": [
                                "/MessTcpPath" // 必须换成自定义的 PATH，需要和分流的一致
                            ]
                        }
                    }
                }
            }
        },
        {
            "port": MessWsPort,
            "listen": "127.0.0.1",
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {
                        "id": "MessWsID", // 填写你的 UUID
                        "level": 0,
                        "email": "admin@DomainName"
                    }
                ]
            },
            "streamSettings": {
                "network": "ws",
                "security": "none",
                "wsSettings": {
                    "acceptProxyProtocol": true, // 提醒：若你用 Nginx/Caddy 等反代 WS，需要删掉这行
                    "path": "/MessWsPath" // 必须换成自定义的 PATH，需要和分流的一致
                }
            }
        }
    ],
    "outbounds": [
        {
            "protocol": "freedom"
        }
    ]
}
