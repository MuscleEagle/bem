{
    "log": {
        "access": "/var/log/v2ray/access.log",
        "error": "/var/log/v2ray/error.log",
        "loglevel": "warning"
    },
    "inbounds": [{
            "port": YourPortWS,
            "listen": "127.0.0.1",
            "protocol": "vmess",
            "settings": {
                "clients": [{
                        "id": "YourIdWS",
                        "level": 1,
                        "alterId": 64
                    }
                ]
            },
            "streamSettings": {
                "network": "ws",
                "wsSettings": {
                    "path": "/YourPathWS"
                }
            }
        }, {
            "port": YourPortKCP,
            "protocol": "vmess",
            "settings": {
                "clients": [{
                        "id": "YourIdKcp",
                        "level": 1,
                        "alterId": 64
                    }
                ]
            },
            "streamSettings": {
                "network": "mkcp",
                "kcpSettings": {
                    "uplinkCapacity": 5,
                    "downlinkCapacity": 100,
                    "congestion": true,
                    "header": {
                        "type": "wechat-video"
                    }
                }
            }
        }
    ],
    "outbounds": [{
         "protocol": "freedom",
         "settings": {}
    }]
}
